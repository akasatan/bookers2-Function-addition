<div class="container">
  <div class="row">
    <div class="col-6 mx-auto">
    <%= form_with url: search_users_path, method: :get, local: true do |f| %>
      <%= f.search_field :search %>
      <%= f.select :model_choose, [["User", "1"],["Book","2"]] %>
      <%= f.select :option, [["完全一致", "1"],["前方一致","2"],["後方一致","3"],["部分一致","4"]] %>
      <%= f.submit '検索' %>
    <% end %>
    </div>
  </div>
</div>